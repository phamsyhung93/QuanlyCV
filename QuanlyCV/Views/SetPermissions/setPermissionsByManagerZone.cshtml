

<div class="main-grid">
    <div class="agile-grids">
        <!-- tables -->

        <div class="table-heading">
            <h2>Sét Quyền</h2>
            <h4 style="color:darkred">@TempData["error"]</h4>
        </div>
        <a onclick="backSetPermissions(this)">Quay về</a>
        <select id="list-permissions-by-managerZone" style="padding:5px;" onchange="listPermissionsByManagerZone(this)">
                <option value="0">Khu Vực Quản Lý</option>
                @foreach (var item in ViewBag.ManagerZone)
                {
                    <option value="@item.ManagerZoneId" data-id="@ViewBag.Role.RoleId">@item.DisplayNameController</option>
                }
            </select>
        <span class="text-right">Xét Quyền cho: @ViewBag.Role.RoleName</span>
        <div class="agile-tables">
            <div class="w3l-table-info tbl-list-Permisstion-by-ManagerZone">
               
            </div>
        </div>
        <!-- //tables -->
    </div>
</div>


